<!DOCTYPE html>
<html>
<head>
  <title>School Information System</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= stylesheet_link_tag    "jquery-ui-1.8.21.custom" %>
  <%= javascript_include_tag "application" %>
  <%= javascript_include_tag 'jquery-1.7.2.min.js', 'jquery-ui-1.8.21.custom.min.js' %>
  <%= javascript_include_tag 'nested_form','jquery', 'jquery-ui', 'jquery.delayed-observer', 'application' %>
  <%#= javascript_include_tag  %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tags %>

  <link type="text/css" href="css/ui-lightness/jquery-ui-1.8.22.custom.css" rel="stylesheet" />

  <script type="text/javascript">
      $(function(){
          // Tabs
          $('#tabs').tabs();
      });
  </script>
  <%= render 'layouts/shim' %>
</head>
<body id="backgound-body-image">
<div class="ui-accordion-header-active">
  <%= render 'layouts/header' %>
</div>
<% if notice %>
    <p class="notice"> <%= notice %></p>
<% end %>
<% if alert %>
    <p class="alert reduce-border"> <%= alert %></p>
<% end %>
<%= render 'layouts/sidebar' %>
<%#= render 'layouts/sidebar' %>
<div class="container back-color">
  <%= yield %>
  </div>
<%= render 'layouts/footer' %>
</body>
</html>
